<h1>Currency</h1>

<table class="table">
  <thead>
  <tr>
    <th>Country</th>
    <th>Name</th>
    <th>Code</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @currency.each do |currency| %>
    <tr>
      <td><%= currency.country %></td>
      <td><%= currency.name %></td>
      <td><%= currency.code %></td>
   </tr>
  <% end %>
  </tbody>
</table>
